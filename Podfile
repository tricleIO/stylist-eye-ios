def shared_pods
    pod 'AlamofireObjectMapper'
    pod 'Alamofire'
    pod 'JSQMessagesViewController'
    pod 'Kingfisher'
    pod 'KVNProgress'
    pod 'ObjectMapper'
    pod 'SnapKit'

    pod 'Fabric'
    pod 'Crashlytics'
end

target ‘StylistEye’ do
    shared_pods
end

